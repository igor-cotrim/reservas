{
  "kind": "collectionType",
  "collectionName": "reservations",
  "info": {
    "singularName": "reservation",
    "pluralName": "reservations",
    "displayName": "Reservation"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "start": {
      "type": "datetime",
      "required": true
    },
    "end": {
      "type": "datetime",
      "required": true
    },
    "isSemester": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "requester": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "disapproved",
        "pending",
        "approved"
      ],
      "required": true,
      "default": "pending"
    },
    "semester": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::semester.semester"
    },
    "ambience": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::ambience.ambience",
      "inversedBy": "reservations"
    }
  }
}
